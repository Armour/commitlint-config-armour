{
  "name": "commitlint-config-armour",
  "version": "1.2.1",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "author": "Chong Guo <armourcy@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Armour/commitlint-config-armour.git"
  },
  "bugs": "https://github.com/Armour/commitlint-config-armour/issues",
  "homepage": "https://github.com/Armour/commitlint-config-armour#readme",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "commitlint",
    "commitlint-config"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "husky": "1.0.0-rc.13"
  },
  "dependencies": {
    "@commitlint/cli": "7.0.0"
  }
}
