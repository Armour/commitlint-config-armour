{
  "name": "commitlint-config-armour",
  "version": "1.2.1",
  "description": "shareable config for commitlint",
  "author": "Chong Guo <armourcy@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@commitlint/cli": "7.6.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "2.3.0"
  },
  "homepage": "https://github.com/Armour/commitlint-config-armour#readme",
  "bugs": "https://github.com/Armour/commitlint-config-armour/issues",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "commitlint",
    "commitlint-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Armour/commitlint-config-armour.git"
  }
}
